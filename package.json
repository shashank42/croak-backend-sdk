{
  "name": "croak-backend-sdk",
  "version": "1.0.2",
  "description": "OpenAPI client for croak-backend-sdk",
  "author": "sh0x",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shashank42/croak-backend-sdk.git"
  },
  "keywords": [
    "Non-fungible token",
    "NFT",
    "Minting",
    "Ethereum",
    "BAYC",
    "Digital art",
    "Crypto",
    "Wallet"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shashank42/croak-backend-sdk/issues"
  },
  "homepage": "https://github.com/shashank42/croak-backend-sdk#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "files": [
    "lib/**/*"
  ]
}
